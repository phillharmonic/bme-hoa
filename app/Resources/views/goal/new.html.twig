{% extends 'AdminLTE.html.twig' %}
{% block title %}New Goal - Admin{% endblock %}
{% block pageheader %}<h1><span class="glyphicon glyphicon-file">&nbsp;</span>Admin | <small><small>SHOW AGENDA</small></small></h1><p>Brandy Mill Estates Homeowners Association</p>{% endblock %}
{% block content %}
    
    <div class="content-wrapper"><!-- content wrapper; contains page content -->

        <section class="content-header"><!-- content header -->
          <h1><!-- heading -->
            Goals
            <small>INDEX</small>
          </h1><!--/heading -->
          <ol class="breadcrumb"><!--breadcrumb -->
            <li class="breadcrumb-item"><a href="{{ path('home') }}">Home</a></li>
            <li class="breadcrumb-item"><a href="#">Admin</a></li>
            <li class="breadcrumb-item active">User</li>
          </ol><!--/breadcrumb -->
        </section><!--/content header -->

        <section class="content"><!-- main content -->
            
            <div class="box box-default"><!-- FULL ROW EXAMPLE -->
                <div class="box-header with-border">
                    <h3 class="box-title">Box One <small>FULL ROW</small></h3>
                    <div class="box-tools pull-right">
                        <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                        <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-remove"></i></button>
                    </div>
                </div><!-- /.box-header -->
                <div class="box-body"><!--.box-body start -->

                </div><!-- /.box-body end-->
                <div class="box-footer">
                  Box Footer
                </div>
            </div><!--/ FULL ROW EXAMPLE -->
      
            <div class="row"><!-- Row of columns-->
          
                <div class="col-md-6"><!-- /.col (left) -->

                     <!-- Form content-->
                    {% form_theme form 'form/AdminLTE_layout.html.twig' %}
                    <div class="box box-success  collapsed-box"><!-- Horizontal Form -->
                        <div class="box-header with-border">
                            <h3 class="box-title">Create New</h3>
                            <div class="box-tools pull-right">
                                <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                                <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-remove"></i></button>
                            </div>
                        </div><!-- box header End -->
                        {{ form_start(form) }}<!-- Form Start -->
                        <div class="box-body"><!-- box body start -->
                            {{ form_widget(form) }}
                        </div><!--/box body end -->
                        {{ form_end(form) }}<!--/Form End -->
                    </div><!-- /Form content -->
                    
                </div><!-- /.col (left) -->
        
                <div class="col-md-6"><!-- /col (right) -->
                    
                    <div class="box box-primary">
                        <div class="box-header">
                          <h3 class="box-title">Box Two</h3>
                        </div>
                        <div class="box-body">

                      </div><!-- /.box-body -->
                    </div><!-- /.box -->
                    
                </div><!-- /.col (right) -->
        
            </div><!-- /.row of columns-->
            
            <div class="row"><!-- Row of columns-->
          
                <div class="col-md-6"><!-- /.col (left) -->

                    <div class="box box-danger">
                        <div class="box-header">
                          <h3 class="box-title">Box One</h3>
                        </div>
                        <div class="box-body">

                        </div><!-- /.box-body -->
                    </div><!-- /.box -->
                    
                </div><!-- /.col (left) -->
        
                <div class="col-md-6"><!-- /col (right) -->
                    
                    <div class="box box-primary">
                        <div class="box-header">
                          <h3 class="box-title">Box Two</h3>
                        </div>
                        <div class="box-body">

                      </div><!-- /.box-body -->
                    </div><!-- /.box -->
                    
                </div><!-- /.col (right) -->
        
            </div><!-- /.row of columns-->
            
            <!-- FULL ROW EXAMPLE -->
            <div class="box box-default">
                <div class="box-header with-border">
                    <h3 class="box-title">Box One <small>FULL ROW</small></h3>
                    <div class="box-tools pull-right">
                        <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                        <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-remove"></i></button>
                    </div>
                </div><!-- /.box-header -->
                <div class="box-body"><!--.box-body start -->

                </div><!-- /.box-body end-->
                <div class="box-footer">
                  Box Footer
                </div>
            </div><!-- /.box -->

        </section><!--/main content -->
        
    </div><!--/content wrapper; contains page content -->
    
{% endblock %}