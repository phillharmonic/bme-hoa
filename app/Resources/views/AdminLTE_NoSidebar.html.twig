{% extends 'alt-base.html.twig' %}

{% block stylesheets %}    
    {# do not add parent() #}
    <!-- favicon -->
    <link rel="icon" type="image/x-icon" href="{{ asset('bundles/main/images/house.png') }}" />
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <!-- jvectormap -->
    <link rel="stylesheet" href=" {{ asset('AdminLTE/plugins/jvectormap/jquery-jvectormap-1.2.2.css') }} ">
    <!-- bootstrap datepicker -->
    <link rel="stylesheet" href="{{ asset('AdminLTE/plugins/datepicker/datepicker3.css') }}">
    <!-- Theme style -->
    <link rel="stylesheet" href=" {{ asset('AdminLTE/dist/css/AdminLTE.min.css') }} ">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href=" {{ asset('AdminLTE/dist/css/skins/skin-black-light.min.css') }} ">
    <!-- Bootstrap 3.3.7 -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <!-- DataTables -->
    <link rel="stylesheet" href="{{asset('AdminLTE/plugins/datatables/dataTables.bootstrap.css')}}">
    <!-- Page styles -->
    <link href="{{ asset('bundles/main/css/AdminLTE-extras.css') }}" type="text/css" rel="stylesheet" />
    <style>
        .content-wrapper {
            background-color: #eeeeee;
        }
        .fill-gap {
            padding-top: 50px;
        }

        .dropdown-submenu {
            position: relative;
        }

        .dropdown-submenu .dropdown-menu {
            top: 0;
            left: 100%;
            margin-top: -1px;
        }
        .footer-pageinfo {
            padding-top: 30px;  padding-left: 20px;
        } 
        /* On small screens, set height to 'auto' for sidenav and grid */
        @media screen and (max-width: 767px) {
            .sidenav {
                height: auto;
                padding: 15px;
            }
        }
        .modal {
            text-align: center;
            padding: 0!important;
        }
        .modal:before {
            content: '';
            display: inline-block;
            height: 100%;
            vertical-align: middle;
            margin-right: -4px; /* Adjusts for spacing */
        }
        .modal-dialog {
            display: inline-block;
            text-align: left;
            vertical-align: middle;
        }
    </style>
{% endblock %}

{% block javascripts %}
    {# do not add parent() #}
    <!-- jQuery MUST be placed first.  jQuery 3.1.1 -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <!-- Bootstrap 3.3.6 - NEWER versions BREAK AdminLTE  -->
    <script src="https://almsaeedstudio.com/themes/AdminLTE/bootstrap/js/bootstrap.min.js"></script>
    <!-- BME HOA Base Script -->
    <script src=" {{ asset('bundles/main/js/base.js') }} "></script>
    <!-- FastClick -->
    <script src=" {{ asset('AdminLTE/plugins/fastclick/fastclick.js') }} "></script>
    <!-- AdminLTE App -->
    <script src=" {{ asset('AdminLTE/dist/js/app.min.js') }} "></script>
    <!-- Sparkline -->
    <script src=" {{ asset('AdminLTE/plugins/sparkline/jquery.sparkline.min.js') }} "></script>
    <!-- jvectormap -->
    <script src=" {{ asset('AdminLTE/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js') }} "></script>
    <script src=" {{ asset('AdminLTE/plugins/jvectormap/jquery-jvectormap-world-mill-en.js') }} "></script>
    <!-- bootstrap datepicker -->
    <script src=" {{ asset('AdminLTE/plugins/datepicker/bootstrap-datepicker.js') }} "></script>
    <!-- SlimScroll 1.3.0 -->
    <script src=" {{ asset('AdminLTE/plugins/slimScroll/jquery.slimscroll.min.js') }} "></script>
    <!-- ChartJS 1.0.1 -->
    <script src=" {{ asset('AdminLTE/plugins/chartjs/Chart.min.js') }} "></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="https://almsaeedstudio.com/themes/AdminLTE/dist/js/pages/dashboard2.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="https://almsaeedstudio.com/themes/AdminLTE/dist/js/demo.js"></script>
    <!-- DataTables -->
    <script src="{{asset('AdminLTE/plugins/datatables/jquery.dataTables.min.js')}}"></script>
    <script src="{{asset('AdminLTE/plugins/datatables/dataTables.bootstrap.min.js')}}"></script>
    <!-- page script -->
    <script>
      $(function () {
        $("#example1").DataTable();
        $('#example2').DataTable({
          "paging": true,
          "lengthChange": false,
          "searching": false,
          "ordering": true,
          "info": true,
          "autoWidth": false
        });
      });
    </script> 
{% endblock %}

{% block title %}Experimental Page{% endblock %}
{# BODY #}
{% block body %} 
    {# TOP NAV BAR #}
    <nav class="navbar navbar-inverse navbar-fixed-top" style="z-index:9999;">
        <div class="container-fluid">
            {% block nav %} {{ include('menus/bootstrap-main-menu.html.twig') }} {% endblock %}
        </div>
    </nav>
    {# JUMBOTRON #}    
    <div class="fill-gap"></div>
    <div class="jumbotron">
        <div class="container text-center">
            {% block pageheader %}<h1><span class="glyphicon glyphicon-file">&nbsp;</span>Admin | <small><small>DASHBOARD</small></small></h1><p>Brandy Mill Estates Homeowners Association</p>{% endblock %}
            {% block alerts %}
                {{ include('pages/alerts.twig') }}
            {% endblock %}
        </div>
    </div>   
        
    <div class="col-md-1"></div>
    
    <div class="col-md-10">
        <div class="wrapper" style='background-color: #eeeeee;'> 
        
        {% block content %} 
            
        {% endblock %}
        
        <!-- Admin Footer -->
            {% block adminFooter %} {% include('admin/AdminLTE/footer.html.twig') %} {% endblock %}
        <!-- /.admin-footer -->

        
        </div><!-- ./wrapper -->
    </div> 
        
    <div class="col-md-1"></div>
    
    <footer class="container-fluid col-md-12">
        {% block footer %}{{ include('pages/alt-footer.html.twig') }} {% endblock %} 
    </footer>
        
    <!-- Settings SideMenue -->
        {% block adminSettings %} {% include('admin/AdminLTE/settings.html.twig') %} {% endblock %}
    <!-- /.settings-side-menu -->    
        
{% endblock %}



    