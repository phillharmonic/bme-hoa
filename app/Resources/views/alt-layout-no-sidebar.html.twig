{% extends 'alt-base.html.twig' %}
{% block stylesheets %}{{ parent() }}{% endblock %}
{% block javascripts %}{{ parent() }}{% endblock %}
{% block title %}Experimental Page{% endblock %}

{% block body %} 
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container-fluid">
            {% block nav %} {{ include('menus/bootstrap-main-menu.html.twig') }} {% endblock %}
        </div>
    </nav>
        
    <div class="fill-gap"></div>
    <div class="jumbotron">
        <div class="container text-center">
            {% block pageheader %}
                <h1>Generic Headline | </h1>      
                <p>generic tag line</p>
            {% endblock %}
            {% block alerts %}
                {{ include('pages/alerts.twig') }}
            {% endblock %}
        </div>
    </div>    

    <div class="container">
        <div class="starter-template">
            {% block content %} {% endblock %}
        </div>
    </div>
        
    <footer class="container-fluid">
        {% block footer %}{{ include('pages/alt-footer.html.twig') }} {% endblock %} 
    </footer>
        
{% endblock %}